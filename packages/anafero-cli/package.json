{
  "name": "@riboseinc/anafero-cli",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "yarn@4.5.0",
  "bin": {
    "build-site": "build-site.mjs"
  },
  "scripts": {
    "cbp": "yarn clean-all; yarn compile; yarn build-generator-builder && yarn build-generator -- --debug && sed 's#^import devtools#//import devtools#' build-site.mjs > ._tmp && mv ._tmp build-site.mjs && npm pack",
    "bp": "rm -f riboseinc-anafero-*.tgz; yarn build-generator -- --debug && npm pack",
    "build-generator-builder": "yarn esbuild build-generator.mts --log-level=debug --packages=external --platform=node --target=node18 --minify=false --bundle --format=esm --outfile=build-generator.mjs",
    "build-generator": "yarn node build-generator.mjs --",
    "clean-all": "rm -f anafero-*.tgz build-generator.mjs build-site.mjs bootstrap.js *.css",
    "compile": "tsc --outdir compiled"
  },
  "dependencies": {
    "@effect/schema": "^0.74.0",
    "@riboseinc/isogit-lfs": "0.2.0",
    "esbuild-wasm": "^0.24.0",
    "immer": "^10.1.1",
    "node-fetch": "^3.3.2",
    "xpath": "^0.0.34",
    "yoga-wasm-web": "0.3.3"
  },
  "devDependencies": {
    "@aws-crypto/sha256-universal": "^2.0.0",
    "@effect/cli": "^0.44.4",
    "@effect/platform": "^0.65.4",
    "@effect/platform-node": "^0.60.4",
    "@effect/printer": "^0.36.2",
    "@effect/printer-ansi": "^0.36.2",
    "@effect/typeclass": "^0.27.2",
    "@hyrious/esbuild-plugin-commonjs": "^0.2.4",
    "@inkjs/ui": "2.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-helmet": "^6",
    "@xmldom/xmldom": "^0.9.3",
    "anafero": "workspace:~",
    "effect": "^3.10.15",
    "esbuild": "^0.24.0",
    "import-mapper": "0.0.4",
    "ink": "5.0.1",
    "isomorphic-git": "^1.27.1",
    "jsdom": "^20.0.0",
    "linkedom": "^0.18.5",
    "prosemirror-model": "^1.22.3",
    "prosemirror-schema-list": "^1.4.1",
    "prosemirror-tables": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "typescript": "^5.6.2"
  }
}
